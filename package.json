{
  "name": "@sisense/pivot2-ui",
  "version": "1.12.5",
  "description": "An awesome implementaion for pivot table",
  "main": "dist/main.js",
  "files": [
    "dist/",
    "client/",
    "tests/",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "start": "yarn run styleguide:watch",
    "server": "node ./server/app.js",
    "build": "yarn run webpack",
    "build:prod": "yarn run copy:commonjs & yarn run build --env.prod",
    "copy:commonjs": "cross-env NODE_ENV=commonjs babel src --out-dir dist/commonjs -s",
    "webpack": "webpack --config webpack.config.js",
    "styleguide": "yarn run styleguidist build",
    "styleguide:watch": "yarn run styleguidist server",
    "styleguidist": "styleguidist --config styleguide.config.js",
    "test": "karma start ./tests/unit/karma.conf.js --addSourceMap",
    "test:watch": "karma start ./tests/unit/karma.conf.js --auto-watch --no-single-run --addSourceMap",
    "test:gitlab": "karma start ./tests/unit/karma.conf.js --addCoverage --testForGitlab",
    "test:coverage": "karma start ./tests/unit/karma.conf.js --addCoverage",
    "test:node": "mocha --opts ./tests/unit/mocha.node.opts",
    "test:node:watch": "mocha --opts ./tests/unit/mocha.node.opts --watch",
    "lint": "eslint ./**/*.{ts,tsx}",
    "lint:watch": "esw -w ./**/*.{ts,tsx}",
    "flow": "flow",
    "tsc": "tsc",
    "docs": "jsdoc -c ./jsdoc.json --debug",
    "prepublishOnly": "yarn run build:prod"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jsoneditor": "^5.28.1",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/socket.io-client": "^1.4.32",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "autoprefixer": "^7.2.5",
    "better-docs": "^1.4.7",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "csv-stream": "^0.1.3",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "18.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsdoc": "^3.3.1",
    "eslint-plugin-jsx": "^0.0.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.7.0",
    "eslint-watch": "6.0.1",
    "file-loader": "^4.3.0",
    "flow-bin": "^0.63.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdoc": "^3.6.3",
    "jsdoc-plugin-typescript": "^2.0.5",
    "jsoneditor": "^9.5.6",
    "karma": "^2.0.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-teamcity-reporter": "^1.1.0",
    "karma-webpack": "4.0.2",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.7.2",
    "object-hash": "^1.1.5",
    "postcss-loader": "^3.0.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-styleguidist": "^10.2.1",
    "react-test-renderer": "^16.3.2",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^6.0.6",
    "sinon": "^4.2.1",
    "sinon-chai": "^2.14.0",
    "style-loader": "^0.19.1",
    "ts-loader": "^6.2.1",
    "tui-jsdoc-template": "^1.2.2",
    "typescript": "^3.6.3",
    "webpack": "4.41.2",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.10",
    "webpack-notifier": "^1.5.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.7.0",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bignumber.js": "^7.2.1",
    "classnames": "^2.2.3",
    "dom-css": "2.1.0",
    "events": "^3.0.0",
    "prop-types": "^15.6.1",
    "raf": "3.4.0",
    "socket.io-client": "^2.1.1",
    "uuid": "^3.2.1"
  },
  "peerDependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "publishConfig": {
    "registry": "https://artifactory.sisense.com/artifactory/api/npm/sisense-node-modules/"
  }
}
